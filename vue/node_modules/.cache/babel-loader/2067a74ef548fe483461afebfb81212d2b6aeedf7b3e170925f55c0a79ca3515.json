{"ast":null,"code":"import \"core-js/modules/es.array.push.js\";\nimport { User, Lock } from \"@element-plus/icons-vue\";\nimport request from \"@/utils/request\";\nexport default {\n  name: \"Login\",\n  components: {\n    Lock\n  },\n  data() {\n    return {\n      form: {},\n      rules: {\n        username: [{\n          required: true,\n          message: '请输入用户名',\n          trigger: 'blur'\n        }\n        // {min: 3, max: 10, message: '长度在 3 到 10 个字符', trigger: 'blur'}\n        ],\n        password: [{\n          required: true,\n          message: '请输入密码',\n          trigger: 'blur'\n        }, {\n          min: 3,\n          max: 10,\n          message: '长度在 3 到 10 个字符',\n          trigger: 'blur'\n        }]\n      }\n    };\n  },\n  methods: {\n    login() {\n      request.post(\"/user/login\", this.form).then(res => {\n        if (res.code === '0') {\n          this.$message({\n            type: 'success',\n            message: '登录成功'\n          });\n          this.$router.push('/'); //登录成功后跳转到主页\n        } else {\n          this.$message({\n            type: 'error',\n            message: res.msg\n          });\n        }\n      });\n    }\n  }\n};","map":{"version":3,"names":["User","Lock","request","name","components","data","form","rules","username","required","message","trigger","password","min","max","methods","login","post","then","res","code","$message","type","$router","push","msg"],"sources":["D:\\CODING\\IDEA-coding\\SpringDemo\\SpringBootVue\\vue\\src\\views\\Login.vue"],"sourcesContent":["<script>\r\n\r\nimport {User, Lock} from \"@element-plus/icons-vue\"\r\nimport request from \"@/utils/request\";\r\nexport default {\r\n  name: \"Login\",\r\n  components: {Lock},\r\n  data(){\r\n    return {\r\n      form:{},\r\n      rules:{\r\n        username:[\r\n          {required: true, message: '请输入用户名', trigger: 'blur'},\r\n          // {min: 3, max: 10, message: '长度在 3 到 10 个字符', trigger: 'blur'}\r\n        ],\r\n        password:[\r\n       