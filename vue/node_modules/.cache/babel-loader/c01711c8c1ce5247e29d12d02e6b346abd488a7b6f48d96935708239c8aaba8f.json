{"ast":null,"code":"import \"core-js/modules/es.array.push.js\";\nimport { User, Lock } from \"@element-plus/icons-vue\";\nimport request from \"@/utils/request\";\nexport default {\n  name: \"Login\",\n  components: {\n    Lock\n  },\n  data() {\n    return {\n      form: {},\n      rules: {\n        username: [{\n          required: true,\n          message: '请输入用户名',\n          trigger: 'blur'\n        }\n        // {min: 3, max: 10, message: '长度在 3 到 10 个字符', trigger: 'blur'}\n        ],\n        password: [{\n          required: true,\n          message: '请输入密码',\n          trigger: 'blur'\n        }\n        // {min: 3, max: 10, message: '长度在 3 到 10 个字符', trigger: 'blur'}\n        ]\n      }\n    };\n  },\n  created() {\n    sessionStorage.removeItem('user');\n  },\n  methods: {\n    login() {\n      this.$refs.form.validate(valid => {\n        if (!valid) {\n          return false;\n        }\n      });\n      request.post(\"/user/login\", this.form).then(res => {\n        if (res.code === '0') {\n          this.$message({\n            type: 'success',\n            message: '登录成功'\n          });\n          sessionStorage.setItem('user', JSON.stringify(res.data)); //缓存用户信息\n          this.$router.push('/'); //登录成功后跳转到主页\n          // console.log(sessionStorage)\n        } else {\n          this.$message({\n            type: 'error',\n            message: res.msg\n          });\n        }\n      });\n    }\n  }\n};","map":{"version":3,"names":["User","Lock","request","name","components","data","form","rules","username","required","message","trigger","password","created","sessionStorage","removeItem","methods","login","$refs","validate","valid","post","then","res","code","$message","type","setItem","JSON","stringify","$router","push","msg"],"sources":["D:\\CODING\\IDEA-coding\\SpringDemo\\SpringBootVue\\vue\\src\\views\\Login.vue"],"sourcesContent":["<script>\r\n\r\nimport {User, Lock} from \"@element-plus/icons-vue\"\r\nimport request from \"@/utils/request\";\r\nexport default {\r\n  name: \"Login\",\r\n  components: {Lock},\r\n  data(){\r\n    return {\r\n      form:{},\r\n      rules:{\r\n        username:[\r\n          {required: true, message: '请输入用户名', trigger: 'blur'},\r\n          // {min: 3, max: 10, message: '长度在 3 到 10 个字符', trigger: 'blur'}\r\n        ],\r\n        password:[\r\n          {required: true, message: '请输入密码', trigger: 'blur'},\r\n          // {min: 3, max: 10, message: '长度在 3 到 10 个字符', trigger: 'blur'}\r\n        ]\r\n      }\r\n    }\r\n  },\r\n  created(){\r\n    sessionStorage.removeItem('user')\r\n  },\r\n  methods:{\r\n    login(){\r\n      this.$refs.form.validate(valid => {\r\n        if (!valid) {\r\n          return false;\r\n        }\r\n      })\r\n      request.post(\"/user/login\",this.form).then(res=>{\r\n        if(res.code === '0'){\r\n          this.$message({\r\n            type: 'success',\r\n            message: '登录成功'\r\n          })\r\n          sessionStorage.setItem('user',JSON.stringify(res.data)) //缓存用户信息\r\n          this.$router.push('/')  //登录成功后跳转到主页\r\n          // console.log(sessionStorage)\r\n        }else{\r\n          this.$message({\r\n            type: 'error',\r\n            message: res.msg\r\n          });\r\n        }\r\n      })\r\n    }\r\n  }\r\n}\r\n</script>\r\n\r\n\r\n<template>\r\n  <div style=\"width: 100%; height:100vh;background-color: white;overflow: hidden\">\r\n    <div style=\"width: 400px;margin: 150px auto\">\r\n      <div style=\"color: #cccccc;font-size: 30px;text-align: center;padding: 30px 0\">欢迎登录</div>\r\n      <el-form ref=\"form\" :model=\"form\" size=\"normal\" :rules=\"rules\">\r\n        <el-form-item prop=\"username\">\r\n          <el-input prefix-icon=User v-model=\"form.username\" placeholder=\"请输入用户名\" />\r\n        </el-form-item>\r\n        <el-form-item prop=\"password\">\r\n          <el-input prefix-icon=Lock v-model=\"form.password\"  show-password placeholder=\"请输入密码\"/>\r\n        </el-form-item>\r\n        <el-form-item>\r\n          <el-button type=\"primary\" style=\"width: 100%\" @click=\"login\">登录</el-button>\r\n        </el-form-item>\r\n      </el-form>\r\n    </div>\r\n  </div>\r\n</template>\r\n\r\n\r\n\r\n<style scoped>\r\n\r\n</style>"],"mappings":";AAEA,SAAQA,IAAI,EAAEC,IAAI,QAAO,yBAAwB;AACjD,OAAOC,OAAM,MAAO,iBAAiB;AACrC,eAAe;EACbC,IAAI,EAAE,OAAO;EACbC,UAAU,EAAE;IAACH;EAAI,CAAC;EAClBI,IAAIA,CAAA,EAAE;IACJ,OAAO;MACLC,IAAI,EAAC,CAAC,CAAC;MACPC,KAAK,EAAC;QACJC,QAAQ,EAAC,CACP;UAACC,QAAQ,EAAE,IAAI;UAAEC,OAAO,EAAE,QAAQ;UAAEC,OAAO,EAAE;QAAM;QACnD;QAAA,CACD;QACDC,QAAQ,EAAC,CACP;UAACH,QAAQ,EAAE,IAAI;UAAEC,OAAO,EAAE,OAAO;UAAEC,OAAO,EAAE;QAAM;QAClD;QAAA;MAEJ;IACF;EACF,CAAC;EACDE,OAAOA,CAAA,EAAE;IACPC,cAAc,CAACC,UAAU,CAAC,MAAM;EAClC,CAAC;EACDC,OAAO,EAAC;IACNC,KAAKA,CAAA,EAAE;MACL,IAAI,CAACC,KAAK,CAACZ,IAAI,CAACa,QAAQ,CAACC,KAAI,IAAK;QAChC,IAAI,CAACA,KAAK,EAAE;UACV,OAAO,KAAK;QACd;MACF,CAAC;MACDlB,OAAO,CAACmB,IAAI,CAAC,aAAa,EAAC,IAAI,CAACf,IAAI,CAAC,CAACgB,IAAI,CAACC,GAAG,IAAE;QAC9C,IAAGA,GAAG,CAACC,IAAG,KAAM,GAAG,EAAC;UAClB,IAAI,CAACC,QAAQ,CAAC;YACZC,IAAI,EAAE,SAAS;YACfhB,OAAO,EAAE;UACX,CAAC;UACDI,cAAc,CAACa,OAAO,CAAC,MAAM,EAACC,IAAI,CAACC,SAAS,CAACN,GAAG,CAAClB,IAAI,CAAC,GAAE;UACxD,IAAI,CAACyB,OAAO,CAACC,IAAI,CAAC,GAAG,GAAG;UACxB;QACF,CAAC,MAAI;UACH,IAAI,CAACN,QAAQ,CAAC;YACZC,IAAI,EAAE,OAAO;YACbhB,OAAO,EAAEa,GAAG,CAACS;UACf,CAAC,CAAC;QACJ;MACF,CAAC;IACH;EACF;AACF","ignoreList":[]},"metadata":{},"sourceType":"module","externalDependencies":[]}