{"ast":null,"code":"import request from \"@/utils/request\";\nexport default {\n  name: \"ToDoList\",\n  components: {},\n  data() {\n    return {\n      form: {},\n      dialogVisible: false,\n      search: '',\n      currentPage: 1,\n      pageSize: 10,\n      total: 0,\n      tableData: []\n    };\n  },\n  created() {\n    this.load();\n  },\n  methods: {\n    load() {\n      //从路由参数中获取id\n      const userId = this.$route.query.id;\n      console.log(userId);\n      request.get(\"/event\", {\n        params: {\n          pageNum: this.currentPage,\n          pageSize: this.pageSize,\n          search: this.search,\n          userId: userId\n        }\n      }).then(res => {\n        console.log(res);\n        this.tableData = res.data.records;\n        this.total = res.data.total;\n      });\n    },\n    add() {\n      this.dialogVisible = true;\n      this.form = {};\n    },\n    save() {\n      if (this.form.id) {\n        //有id是更新，没有是新增\n        request.put(\"/event\", this.form).then(res => {\n          console.log(res);\n          if (res.code === '0') {\n            this.$message({\n              type: 'success',\n              message: '更新成功'\n            });\n          } else {\n            this.$message({\n              type: 'error',\n              message: res.msg\n            });\n          }\n          this.load(); //刷新表格数据\n          this.dialogVisible = false; //关闭弹窗\n        });\n      } else {\n        request.post(\"/event\", this.form).then(res => {\n          console.log(res);\n          this.form.userId = this.$route.query.id;\n          if (res.code === '0') {\n            this.$message({\n              type: 'success',\n              message: '新增成功'\n            });\n          } else {\n            this.$message({\n              type: 'error',\n              message: res.msg\n            });\n          }\n          this.load(); //刷新表格数据\n          this.dialogVisible = false; //关闭弹窗\n        });\n      }\n    },\n    changeStatus(row) {\n      request.put(\"/event/status\", this.form).then(res => {\n        console.log(res);\n        if (res.code === '0') {\n          this.$message({\n            type: 'success',\n            message: '更新成功'\n          });\n        } else {\n          this.$message({\n            type: 'error',\n            message: res.msg\n          });\n        }\n        this.load(); //刷新表格数据\n        this.dialogVisible = false; //关闭弹窗\n      });\n    },\n    handelEdit(row) {\n      this.form = JSON.parse(JSON.stringify(row));\n      this.dialogVisible = true;\n    },\n    handleDelete(id) {\n      console.log(id);\n      request.delete(\"/event/\" + id).then(res => {\n        if (res.code === '0') {\n          this.$message({\n            type: 'success',\n            message: '删除成功'\n          });\n        } else {\n          this.$message({\n            type: 'error',\n            message: res.msg\n          });\n        }\n        this.load(); //刷新表格数据\n        this.dialogVisible = false; //关闭弹窗\n      });\n    },\n    handleSizeChange(PageSize) {\n      //改变当前每页的个数触发\n      this.pageSize = PageSize;\n      this.load();\n    },\n    handleCurrentChange(PageNum) {\n      //改变当前页码出发\n      this.currentPage = PageNum;\n      this.load();\n    }\n  }\n};","map":{"version":3,"names":["request","name","components","data","form","dialogVisible","search","currentPage","pageSize","total","tableData","created","load","methods","userId","$route","query","id","console","log","get","params","pageNum","then","res","records","add","save","put","code","$message","type","message","msg","post","changeStatus","row","handelEdit","JSON","parse","stringify","handleDelete","delete","handleSizeChange","PageSize","handleCurrentChange","PageNum"],"sources":["D:\\CODING\\IDEA-coding\\SpringDemo\\SpringBootVue\\vue\\src\\views\\ToDoList.vue"],"sourcesContent":["<template>\r\n  <el-container>\r\n    <el-header style=\"height: 100px;text-align: left\">\r\n      <div style=\"height: 50px;margin-top: 20px\">\r\n        <el-button type=\"primary\" style=\"margin-left: 10px\" @click=\"add\">新增</el-button>\r\n        <el-button type=\"primary\">导入</el-button>\r\n        <el-button type=\"primary\">导出</el-button>\r\n      </div>\r\n      <!--      搜索区域-->\r\n      <div style=\"height: 30px\">\r\n        <el-input v-model=\"search\" style=\"width: 240px\" placeholder=\"Please input\" clearable/>\r\n        <el-button type=\"primary\" style=\"margin-left: 10px\" @click=\"load\">查询</el-button>\r\n      </div>\r\n\r\n    </el-header>\r\n    <el-main>\r\n      <el-table\r\n          :data=\"tableData\" border style=\"width: 1200px\"\r\n          stripe\r\n      >\r\n        <el-table-column prop=\"id\" label=\"ID\" style=\"width: 100px\" />\r\n        <el-table-column prop=\"eventname\" label=\"事件名\" style=\"width: 200px\" />\r\n        <el-table-column prop=\"description\" label=\"事件描述\" style=\"width: 200px\" />\r\n        <el-table-column prop=\"stday\" label=\"起始日期\" style=\"width: 100px\"/>\r\n        <el-table-column prop=\"enday\" label=\"截止日期\" style=\"width: 100px\"/>\r\n        <el-table-column prop=\"status\" label=\"状态\" style=\"width: 50px\" />\r\n        <el-table-column fixed=\"right\" label=\"Operations\" min-width=\"80px\">\r\n          <template #default = \"scope\">\r\n            <el-popconfirm title=\"Are you sure to delete this?\" @confirm = \"handleDelete(scope.row.id)\">\r\n              <template #reference>\r\n                <el-button link type=\"primary\" size=\"small\" >删除</el-button>\r\n              </template>\r\n            </el-popconfirm>\r\n            <el-button link type=\"primary\" size=\"small\" @click=\"handelEdit(scope.row)\">编辑</el-button>\r\n            <el-button link type=\"primary\" size=\"small\" @click=\"changeStatus(scope.row)\">状态更新</el-button>\r\n          </template>\r\n        </el-table-column>\r\n      </el-table>\r\n      <div class=\"demo-pagination-block\" style=\"margin-top: 10px\">\r\n        <div class=\"demonstration\" style=\"margin-bottom: 10px\">All combined</div>\r\n        <el-pagination\r\n            v-model:current-page=\"currentPage\"\r\n            v-model:page-size=\"pageSize\"\r\n            :page-sizes=\"[1, 2, 4, 8]\"\r\n            :size=\"size\"\r\n            :disabled=\"disabled\"\r\n            :background=\"background\"\r\n            layout=\"total, sizes, prev, pager, next, jumper\"\r\n            :total=\"total\"\r\n            @size-change=\"handleSizeChange \"\r\n            @current-change=\"handleCurrentChange\"\r\n        />\r\n      </div>\r\n    </el-main>\r\n\r\n\r\n  </el-container>\r\n  <el-dialog\r\n      v-model=\"dialogVisible\"\r\n      title=\"Tips\"\r\n      width=\"500px\"\r\n      :before-close=\"handleClose\"\r\n  >\r\n    <el-form :model=\"form\" label-width=\"120px\" style=\"max-width: 600px\">\r\n      <el-form-item label=\"事件名\">\r\n        <el-input v-model=\"form.eventname\" style=\"width: 300px\" />\r\n      </el-form-item>\r\n      <el-form-item label=\"事件描述\">\r\n        <el-input v-model=\"form.description\" style=\"width: 300px\" />\r\n      </el-form-item>\r\n      <el-form-item label=\"起始日期\" >\r\n        <el-date-picker v-model=\"form.stday\" value-format=\"YYYY-MM-DD\" type = \"date\" style=\"width: 80%\"></el-date-picker>\r\n      </el-form-item>\r\n      <el-form-item label=\"截止日期\">\r\n        <el-date-picker v-model=\"form.enday\" value-format=\"YYYY-MM-DD\" type = \"date\" style=\"width: 80%\"></el-date-picker>\r\n      </el-form-item>\r\n      <el-form-item label=\"状态\">\r\n        <el-radio-group v-model=\"form.status\">\r\n          <el-radio value=\"man\">已完成</el-radio>\r\n          <el-radio value=\"women\">进行中</el-radio>\r\n        </el-radio-group>\r\n      </el-form-item>\r\n      <el-form-item>\r\n        <el-button type=\"primary\" @click=\"save\">Create</el-button>\r\n        <el-button>Cancel</el-button>\r\n      </el-form-item>\r\n    </el-form>\r\n    <template #footer>\r\n      <div class=\"dialog-footer\">\r\n        <el-button @click=\"dialogVisible = false\">Cancel</el-button>\r\n        <el-button type=\"primary\" @click=\"dialogVisible = false\">\r\n          Confirm\r\n        </el-button>\r\n      </div>\r\n    </template>\r\n  </el-dialog>\r\n</template>\r\n\r\n<script>\r\nimport request from \"@/utils/request\";\r\n\r\nexport default {\r\n  name: \"ToDoList\",\r\n  components: {\r\n\r\n  },\r\n\r\n  data() {\r\n    return {\r\n      form:{},\r\n      dialogVisible: false,\r\n      search: '',\r\n      currentPage: 1,\r\n      pageSize: 10,\r\n      total: 0,\r\n      tableData: [],\r\n    }\r\n  },\r\n  created() {\r\n    this.load();\r\n  },\r\n  methods: {\r\n    load(){\r\n      //从路由参数中获取id\r\n      const userId = this.$route.query.id;\r\n      console.log(userId)\r\n      request.get(\"/event\",{\r\n        params:{\r\n          pageNum: this.currentPage,\r\n          pageSize: this.pageSize,\r\n          search: this.search,\r\n          userId: userId\r\n        }\r\n      }).then(res => {\r\n        console.log(res)\r\n        this.tableData = res.data.records\r\n        this.total = res.data.total\r\n      })\r\n    },\r\n    add(){\r\n      this.dialogVisible = true\r\n      this.form = {}\r\n    },\r\n    save(){\r\n      if(this.form.id){ //有id是更新，没有是新增\r\n        request.put(\"/event\",this.form).then(res => {\r\n          console.log(res)\r\n          if(res.code === '0'){\r\n            this.$message({\r\n              type: 'success',\r\n              message: '更新成功'\r\n            });\r\n          }else{\r\n            this.$message({\r\n              type: 'error',\r\n              message: res.msg\r\n            });\r\n          }\r\n          this.load() //刷新表格数据\r\n          this.dialogVisible = false  //关闭弹窗\r\n        })\r\n      }else{\r\n        request.post(\"/event\",this.form).then(res => {\r\n          console.log(res)\r\n          this.form.userId=this.$route.query.id;\r\n          if(res.code === '0'){\r\n            this.$message({\r\n              type: 'success',\r\n              message: '新增成功'\r\n            });\r\n          }else{\r\n            this.$message({\r\n              type: 'error',\r\n              message: res.msg\r\n            });\r\n          }\r\n          this.load() //刷新表格数据\r\n          this.dialogVisible = false  //关闭弹窗\r\n        })\r\n      }\r\n    },\r\n    changeStatus(row){\r\n      request.put(\"/event/status\",this.form).then(res => {\r\n        console.log(res)\r\n        if(res.code === '0'){\r\n          this.$message({\r\n            type: 'success',\r\n            message: '更新成功'\r\n          });\r\n        }else{\r\n          this.$message({\r\n            type: 'error',\r\n            message: res.msg\r\n          });\r\n        }\r\n        this.load() //刷新表格数据\r\n        this.dialogVisible = false  //关闭弹窗\r\n      })\r\n    },\r\n    handelEdit(row){\r\n      this.form = JSON.parse(JSON.stringify(row))\r\n      this.dialogVisible = true\r\n    },\r\n    handleDelete(id){\r\n      console.log(id)\r\n      request.delete(\"/event/\"+id).then(res => {\r\n        if(res.code === '0'){\r\n          this.$message({\r\n            type: 'success',\r\n            message: '删除成功'\r\n          });\r\n        }else{\r\n          this.$message({\r\n            type: 'error',\r\n            message: res.msg\r\n          });\r\n        }\r\n        this.load() //刷新表格数据\r\n        this.dialogVisible = false  //关闭弹窗\r\n      })\r\n    },\r\n    handleSizeChange(PageSize) {    //改变当前每页的个数触发\r\n      this.pageSize = PageSize\r\n      this.load()\r\n    },\r\n    handleCurrentChange(PageNum) { //改变当前页码出发\r\n      this.currentPage = PageNum\r\n      this.load()\r\n    },\r\n\r\n  }\r\n}\r\n</script>\r\n"],"mappings":"AAmGA,OAAOA,OAAM,MAAO,iBAAiB;AAErC,eAAe;EACbC,IAAI,EAAE,UAAU;EAChBC,UAAU,EAAE,CAEZ,CAAC;EAEDC,IAAIA,CAAA,EAAG;IACL,OAAO;MACLC,IAAI,EAAC,CAAC,CAAC;MACPC,aAAa,EAAE,KAAK;MACpBC,MAAM,EAAE,EAAE;MACVC,WAAW,EAAE,CAAC;MACdC,QAAQ,EAAE,EAAE;MACZC,KAAK,EAAE,