{"ast":null,"code":"import request from \"@/utils/request\";\nimport user from \"../views/User.vue\";\nexport default {\n  name: \"PersonInfo\",\n  props: ['user'],\n  components: {},\n  data() {\n    return {\n      form: {},\n      dialogVisible: false,\n      PersonInfo: [],\n      user: {}\n    };\n  },\n  created() {\n    let userStr = sessionStorage.getItem('user') || \"{}\";\n    this.user = JSON.parse(userStr);\n    console.log(this.user);\n    this.load();\n    // sessionStorage.removeItem('user')\n  },\n  methods: {\n    handleAvatarSuccess(res) {\n      if (res.code == 0) {\n        this.form.avatar = res.data;\n      }\n    },\n    beforeAvatarUpload(file) {\n      const isImage = file.type.startsWith('image/');\n      const isLt2M = file.size / 1024 / 1024 < 5;\n      if (!isImage) this.$message.error('上传图片');\n      if (!isLt2M) this.$message.error('上传图片不能大于5M');\n      return isImage && isLt2M;\n    },\n    load() {\n      request.get(\"/user/\" + this.user.id).then(res => {\n        console.log(res);\n        this.PersonInfo = [res.data];\n        // console.log(this.PersonInfo)\n      });\n    },\n    handelShow() {\n      this.dialogVisible = true;\n    },\n    handelEdit() {\n      this.form.id = this.user.id;\n      request.put(\"/user\", this.form).then(res => {\n        console.log(res);\n        if (res.code === '0') {\n          this.$message({\n            type: 'success',\n            message: '更新成功'\n          });\n        } else {\n          this.$message({\n            type: 'error',\n            message: res.msg\n          });\n        }\n        this.load();\n        this.dialogVisible = false; //关闭弹窗\n      });\n    }\n  }\n};","map":{"version":3,"names":["request","user","name","props","components","data","form","dialogVisible","PersonInfo","created","userStr","sessionStorage","getItem","JSON","parse","console","log","load","methods","handleAvatarSuccess","res","code","avatar","beforeAvatarUpload","file","isImage","type","startsWith","isLt2M","size","$message","error","get","id","then","handelShow","handelEdit","put","message","msg"],"sources":["D:\\CODING\\IDEA-coding\\SpringDemo\\SpringBootVue\\vue\\src\\views\\PersonInfo.vue"],"sourcesContent":["<template>\r\n  <el-container>\r\n    <el-header style=\"height: 100px;text-align: left\">\r\n      <div style=\"height: 50px;margin-top: 20px\">\r\n        <el-button type=\"primary\" style=\"margin-left: 10px\" @click=\"handelShow\">修改</el-button>\r\n      </div>\r\n\r\n      <el-dropdown>\r\n      <span class=\"el-dropdown-link\">\r\n<!--        菜单管理-->\r\n        <!--          {{user.nickname}}<i class=\"el-icon-arrow-down el-icon&#45;&#45;right\"></i>-->\r\n        <!--        头像-->\r\n        <el-avatar :src=\"this.form?.avatar\" style=\"margin-right: 10px\">\r\n<!--          默认头像-->\r\n          <img v-if=\"!this.form?.avatar\" src=\"../photos/default.jpg\" alt=\"\"/>\r\n        </el-avatar>\r\n        <!--         昵称-->\r\n          {{this.form?.nickname }}\r\n        <el-icon class=\"el-icon--right\">\r\n          <arrow-down />\r\n        </el-icon>\r\n      </span>\r\n        <template #dropdown>\r\n          <el-dropdown-menu>\r\n            <el-dropdown-item @click=\"this.$router.push('/')\">回到主页面</el-dropdown-item>\r\n          </el-dropdown-menu>\r\n        </template>\r\n      </el-dropdown>\r\n\r\n\r\n    </el-header>\r\n    <el-main>\r\n\r\n      <el-table\r\n          :data=\"PersonInfo\" border style=\"width: 1200px\"\r\n          stripe\r\n      >\r\n        <el-table-column prop=\"id\" label=\"ID\" style=\"width: 100px\" />\r\n        <el-table-column prop=\"username\" label=\"用户名\" style=\"width: 200px\" />\r\n        <el-table-column prop=\"nickname\" label=\"昵称\" style=\"width: 200px;\"/>\r\n        <el-table-column prop=\"age\" label=\"年龄\" style=\"width: 100px\"/>\r\n        <el-table-column prop=\"birthday\" label=\"出生日期\" style=\"width: 100px\"/>\r\n        <el-table-column prop=\"sex\" label=\"性别\" style=\"width: 50px\" />\r\n        <el-table-column prop=\"avatar\" label=\"头像\" style=\"width: 50px\" />\r\n        <el-table-column fixed=\"right\" label=\"ToDoList\" min-width=\"80px\">\r\n        </el-table-column>\r\n      </el-table>\r\n      <div class=\"demo-pagination-block\" style=\"margin-top: 10px\">\r\n        <div class=\"demonstration\" style=\"margin-bottom: 10px\">All combined</div>\r\n        <el-pagination\r\n            :disabled=\"disabled\"\r\n            :background=\"background\"\r\n        />\r\n      </div>\r\n    </el-main>\r\n\r\n\r\n  </el-container>\r\n  <el-dialog\r\n      v-model=\"dialogVisible\"\r\n      title=\"Tips\"\r\n      width=\"500px\"\r\n      :before-close=\"handleClose\"\r\n  >\r\n    <el-form :model=\"form\" label-width=\"120px\" style=\"max-width: 600px\">\r\n      <el-form-item label=\"用户名\">\r\n        <el-input v-model=\"form.username\" style=\"width: 300px\" />\r\n      </el-form-item>\r\n      <el-form-item label=\"密码\">\r\n        <el-input v-model=\"form.password\" style=\"width: 300px\"></el-input>\r\n      </el-form-item>\r\n      <el-form-item label=\"昵称\" >\r\n        <el-input v-model=\"form.nickname\" style=\"width: 300px\" />\r\n      </el-form-item>\r\n      <el-form-item label=\"出生日期\">\r\n        <el-date-picker v-model=\"form.birthday\" value-format=\"YYYY-MM-DD\" type = \"date\" style=\"width: 80%\"></el-date-picker>\r\n      </el-form-item>\r\n      <el-form-item label=\"性别\">\r\n        <el-radio-group v-model=\"form.sex\">\r\n          <el-radio value=\"man\">男</el-radio>\r\n          <el-radio value=\"women\">女</el-radio>\r\n        </el-radio-group>\r\n      </el-form-item>\r\n      <el-form-item label=\"地址\">\r\n        <el-input v-model=\"form.address\" style=\"width: 80%\"></el-input>\r\n      </el-form-item>\r\n      <el-form-item label=\"头像地址\">\r\n        <el-upload\r\n            v-model=\"form.avatar\"\r\n            class=\"avatar-uploader\"\r\n            action=\"http://localhost:9090/files/upload\"\r\n            :on-success=\"handleAvatarSuccess\"\r\n            :before-upload=\"beforeAvatarUpload\"\r\n            :show-file-list=\"false\"\r\n        >\r\n          <el-button type=\"primary\">点击上传</el-button>\r\n          <template #tip>\r\n            <div class=\"el-upload__tip\">只能上传jpg/png文件</div>\r\n          </template>\r\n        </el-upload>\r\n      </el-form-item>\r\n    </el-form>\r\n    <template #footer>\r\n      <div class=\"dialog-footer\">\r\n        <el-button @click=\"dialogVisible = false\">取消</el-button>\r\n        <el-button type=\"primary\" @click=\"handelEdit\" >\r\n          确认\r\n        </el-button>\r\n      </div>\r\n    </template>\r\n  </el-dialog>\r\n</template>\r\n\r\n<script>\r\nimport request from \"@/utils/request\";\r\nimport user from \"../views/User.vue\";\r\n\r\nexport default {\r\n  name: \"PersonInfo\",\r\n  props:['user'],\r\n  components: {\r\n\r\n  },\r\n\r\n  data() {\r\n    return {\r\n      form:{},\r\n      dialogVisible: false,\r\n      PersonInfo: [],\r\n      user:{}\r\n    }\r\n  },\r\n  created() {\r\n    let userStr = sessionStorage.getItem('user') || \"{}\"\r\n    this.user = JSON.parse(userStr)\r\n    console.log(this.user)\r\n    this.load();\r\n    // sessionStorage.removeItem('user')\r\n  },\r\n  methods: {\r\n    handleAvatarSuccess(res){\r\n      if(res.code == 0){\r\n        this.form.avatar = res.data;\r\n      }\r\n    },\r\n    beforeAvatarUpload(file){\r\n      const isImage = file.type.startsWith('image/')\r\n      const isLt2M = file.size / 1024 / 1024 < 5;\r\n      if(!isImage) this.$message.error('上传图片');\r\n      if(!isLt2M) this.$message.error('上传图片不能大于5M');\r\n      return isImage && isLt2M;\r\n    },\r\n    load(){\r\n      request.get(\"/user/\" + this.user.id).then(res => {\r\n        console.log(res)\r\n        this.PersonInfo = [res.data]\r\n        // console.log(this.PersonInfo)\r\n      })\r\n    },\r\n    handelShow(){\r\n      this.dialogVisible = true\r\n    },\r\n\r\n    handelEdit(){\r\n      this.form.id = this.user.id;\r\n      request.put(\"/user\",this.form).then(res => {\r\n        console.log(res)\r\n        if(res.code === '0'){\r\n          this.$message({\r\n            type: 'success',\r\n            message: '更新成功'\r\n          });\r\n\r\n        }else{\r\n          this.$message({\r\n            type: 'error',\r\n            message: res.msg\r\n          });\r\n        }\r\n        this.load()\r\n        this.dialogVisible = false  //关闭弹窗\r\n      })\r\n    }\r\n  }\r\n}\r\n</script>\r\n\r\n<style scoped>\r\n\r\n</style>"],"mappings":"AAkHA,OAAOA,OAAM,MAAO,iBAAiB;AACrC,OAAOC,IAAG,MAAO,mBAAmB;AAEpC,eAAe;EACbC,IAAI,EAAE,YAAY;EAClBC,KAAK,EAAC,CAAC,MAAM,CAAC;EACdC,UAAU,EAAE,CAEZ,CAAC;EAEDC,IAAIA,CAAA,EAAG;IACL,OAAO;MACLC,IAAI,EAAC,CAAC,CAAC;MACPC,aAAa,EAAE,KAAK;MACpBC,UAAU,EAAE,EAAE;MACdP,IAAI,EAAC,CAAC;IACR;EACF,CAAC;EACDQ,OAAOA,CAAA,EAAG;IACR,IAAIC,OAAM,GAAIC,cAAc,CAACC,OAAO,CAAC,MAAM,KAAK,IAAG;IACnD,IAAI,CAACX,IAAG,GAAIY,IAAI,CAACC,KAAK,CAACJ,OAAO;IAC9BK,OAAO,CAACC,GAAG,CAAC,IAAI,CAACf,IAAI;IACrB,IAAI,CAACgB,IAAI,CAAC,CAAC;IACX;EACF,CAAC;EACDC,OAAO,EAAE;IACPC,mBAAmBA,CAACC,GAAG,EAAC;MACtB,IAAGA,GAAG,CAACC,IAAG,IAAK,CAAC,EAAC;QACf,IAAI,CAACf,IAAI,CAACgB,MAAK,GAAIF,GAAG,CAACf,IAAI;MAC7B;IACF,CAAC;IACDkB,kBAAkBA,CAACC,IAAI,EAAC;MACtB,MAAMC,OAAM,GAAID,IAAI,CAACE,IAAI,CAACC,UAAU,CAAC,QAAQ;MAC7C,MAAMC,MAAK,GAAIJ,IAAI,CAACK,IAAG,GAAI,IAAG,GAAI,IAAG,GAAI,CAAC;MAC1C,IAAG,CAACJ,OAAO,EAAE,IAAI,CAACK,QAAQ,CAACC,KAAK,CAAC,MAAM,CAAC;MACxC,IAAG,CAACH,MAAM,EAAE,IAAI,CAACE,QAAQ,CAACC,KAAK,CAAC,YAAY,CAAC;MAC7C,OAAON,OAAM,IAAKG,MAAM;IAC1B,CAAC;IACDX,IAAIA,CAAA,EAAE;MACJjB,OAAO,CAACgC,GAAG,CAAC,QAAO,GAAI,IAAI,CAAC/B,IAAI,CAACgC,EAAE,CAAC,CAACC,IAAI,CAACd,GAAE,IAAK;QAC/CL,OAAO,CAACC,GAAG,CAACI,GAAG;QACf,IAAI,CAACZ,UAAS,GAAI,CAACY,GAAG,CAACf,IAAI;QAC3B;MACF,CAAC;IACH,CAAC;IACD8B,UAAUA,CAAA,EAAE;MACV,IAAI,CAAC5B,aAAY,GAAI,IAAG;IAC1B,CAAC;IAED6B,UAAUA,CAAA,EAAE;MACV,IAAI,CAAC9B,IAAI,CAAC2B,EAAC,GAAI,IAAI,CAAChC,IAAI,CAACgC,EAAE;MAC3BjC,OAAO,CAACqC,GAAG,CA